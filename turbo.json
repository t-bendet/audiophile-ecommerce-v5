{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [".env*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": ["dist/**", "generated/**"],
      "env": ["DATABASE_URL"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": [],
      "persistent": true
    },
    "type-check": {
      "cache": true,
      "outputs": []
    },
    "lint": {
      "cache": true,
      "outputs": []
    },
    "db:generate": {
      "cache": false,
      "outputs": ["generated/**"],
      "env": ["DATABASE_URL"]
    },
    "db:push": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:seed": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:deploy": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:setup": {
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:reset": {
      "cache": false,
      "env": ["DATABASE_URL"]
    }
  }
}
